{
    "off-chain-db-credentials": {
        "searchPatterns": [
            "cloudfoundry:off-chain-db",
            "env:off_chain_db",
            "file:/server/config/local-cloudant-config.json"
        ]
    },
    "off-chain-db-credentials-mongo": {
        "searchPatterns": [
            "file:/server/config/local-mongo-config.json"
        ]
    },
    "IBM-certificate-manager-credentials": {
        "searchPatterns": [
            "env:IBM_CERTIFICATE_MANAGER_CONFIG",
            "file:/server/config/local-certmanager-config.json"
        ]
    },
    "off-chain-kvs-credentials": {
        "searchPatterns": [
            "env:OFF_CHAIN_KVS_CONFIG",
            "file:/server/config/local-kvs-config.json"
        ]
    }
}